CREATE OR REPLACE TRIGGER "TRG_PROFDATA_PERIOD" 
BEFORE INSERT OR UPDATE ON profdata
FOR each ROW
declare
  chltype INTEGER;
BEGIN
  BEGIN
    SELECT ch.chltype_id INTO chltype FROM channels ch
      WHERE ch.fid = :NEW.TYPEDATA;
  EXCEPTION
    when others then
      :NEW.Period := null;  
  END;
  IF chltype = 9 THEN
    IF :NEW.Period IS NULL THEN
      SELECT t.custom_period
        INTO :NEW.Period from devices t
       where t.id = :NEW.deviceid;
    END IF;
  ELSE
    IF :NEW.Period IS NULL THEN
      SELECT t.period
        INTO :NEW.Period from devices t
       where t.id = :NEW.deviceid;
    END IF;
  END IF;
END;


SHOW ERRORS 
;

CREATE OR REPLACE TRIGGER "TRG_PROFDATA_SCD_SYNCHR" 
AFTER INSERT OR UPDATE ON profdata
FOR each ROW
BEGIN
    delete profdata_scd p
     where p.deviceid = :old.deviceid
      and p.ardtimestamp = :old.ardtimestamp
      and p.source_id != 2;
END;


SHOW ERRORS 
;

