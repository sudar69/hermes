	,
	"LOGICALID" NUMBER,
	"PHYSICALID" NUMBER,
	"PERIOD" NUMBER,
	"CLIENTTYPE" NUMBER DEFAULT 32  NOT NULL,
	"VIRTUALTYPEDEVICE_ID" NUMBER,
	"KTN" NUMBER DEFAULT 1,
	"KTT" NUMBER DEFAULT 1,
	"SERIALNUM" NUMBER DEFAULT 0,
	"KOEFINDEV" NUMBER DEFAULT 0,
	"DIRECTENERGY" NUMBER DEFAULT 1  NOT NULL,
	"ORDER_ID" NUMBER DEFAULT 0  NOT NULL,
