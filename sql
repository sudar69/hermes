CREATE TABLE "AIRWAYFIDERS"
(
	"FID" NUMBER NOT NULL,
	"FNAME" VARCHAR2(255 BYTE),
	"BASIC_FIDER_ID" NUMBER DEFAULT 0
	,
	"DUBL_FIDER_ID" NUMBER DEFAULT 0
	,
	"AIRWAY_ID" NUMBER DEFAULT 1
	,
	"LENGTH" FLOAT(126) DEFAULT 0
	,
	"ORDER_ID" NUMBER DEFAULT 0  NOT NULL,
	"UN" FLOAT(126) DEFAULT 20  NOT NULL,
	"LENGTH_TO_SALE" FLOAT(126) DEFAULT 0  NOT NULL
)
TABLESPACE	USERS
PCTFREE		10
INITRANS	1
MAXTRANS	255
STORAGE (
	INITIAL		65536
	MINEXTENTS	1
	MAXEXTENTS	2147483645
	BUFFER_POOL	DEFAULT
		)
LOGGING
NOCOMPRESS
NOCACHE
NOPARALLEL
MONITORING
;

