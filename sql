CREATE OR REPLACE TRIGGER "TRG_PROPERTYOWNERS" 
       before insert or delete on propertyowners
       for each row
begin
  if inserting then
    property.putNewPropertyOwners(:new.ownertype_id,:new.property_id);
  else
    property.delPropertyOwners(:old.ownertype_id,:old.property_id);
  end if;
end trg_propertyowners;


