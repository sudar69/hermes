	PRIMARY KEY ("FID") USING INDEX
;

CREATE OR REPLACE TRIGGER "TRG_UP_FORMULAS" 
AFTER UPDATE ON formulas
FOR each ROW
BEGIN
  delete formfracs f
   where f.formula_id = :new.fid; 
  delete calcdata f
   where f.formula_id = :new.fid; 
END;


SHOW ERRORS 
;

ALTER TABLE "FORMULAS" 
 ADD CONSTRAINT "PK_FORMULA_ID"
	PRIMARY KEY ("FID") USING INDEX
;

