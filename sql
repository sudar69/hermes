CREATE OR REPLACE TRIGGER "TRG_DEL_ON_AIRWAYFIDERS" 
AFTER DELETE OR UPDATE ON airwayfiders
FOR each ROW
DECLARE
  devId INTEGER;
BEGIN
  select t.id INTO devId from devices t where t.linktofider = :old.basic_fider_id;
  delete profdata_scd
   where (deviceid = devId or deviceid = devId) 
     and (is_losses = 1 or typedata in(261,262,263,264)) 
     and source_id != 2;
EXCEPTION
  when others then
     null;--raise_application_error(-20000, 'trg_del_on_airwayfiders ' || SQLERRM); 
END;


SHOW ERRORS 
;

ALTER TABLE "AIRWAYFIDERS" 
 ADD CONSTRAINT "PK_AIRWAYFIDERS_ID"
	PRIMARY KEY ("FID") USING INDEX
;

ALTER TABLE "AIRWAYFIDERS" 
 ADD CONSTRAINT "UN_BASIC_FIDER_ID" UNIQUE ("BASIC_FIDER_ID") USING INDEX
;

ALTER TABLE "AIRWAYFIDERS" 
 ADD CONSTRAINT "UN_DUBL_FIDER_ID" UNIQUE ("DUBL_FIDER_ID") USING INDEX
;

ALTER TABLE "AIRWAYTYPES" 
 ADD CONSTRAINT "PK_AIRWAYTYPE_ID"
	PRIMARY KEY ("FID") USING INDEX
;

CREATE OR REPLACE TRIGGER "TRG_DEL_ON_ALTFIDERS" 
AFTER UPDATE OR DELETE ON altfiders FOR EACH ROW
DECLARE
  devId INTEGER;
BEGIN
  select t.id INTO devId from devices t where t.linktofider = :old.fider_id;
  delete profdata_scd p
   where p.deviceid = devId
     and (p.ardtimestamp BETWEEN trunc(:old.datetimestart,'mm')
       and add_months(trunc((:old.datetimeend-1/60/60/24),'mm'),1)
       or p.period >= 2592000  )
       and source_id != 2;
EXCEPTION
  when others then
     null;
--raise_application_error(-20000, 'trg_del_on_airwayfiders ' || SQLERRM);
END;


SHOW ERRORS 
;

ALTER TABLE "ALTFIDERS" 
 ADD CONSTRAINT "PK_ALTFIDER_ID"
	PRIMARY KEY ("FID") USING INDEX
;

ALTER TABLE "ALTFORMULAS" 
 ADD CONSTRAINT "PK_ALTFORMULA_ID"
	PRIMARY KEY ("FID") USING INDEX
;

ALTER TABLE "AR_CFG_COLORS" 
 ADD CONSTRAINT "PK_AR_CFG_COLORS"
	PRIMARY KEY ("USER_ID","MSG_TYPE_ID") USING INDEX
;

ALTER TABLE "AR_CFG_COLUMNS" 
 ADD CONSTRAINT "PK_AR_CFG_COLUMNS"
	PRIMARY KEY ("USER_ID","COLUMN_ID") USING INDEX
;

ALTER TABLE "AR_COLUMNS" 
 ADD CONSTRAINT "PK_AR_COLUMNS"
	PRIMARY KEY ("FID") USING INDEX
;

