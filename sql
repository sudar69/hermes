CREATE OR REPLACE TRIGGER "TRG_UP_FORMOPERANDS" 
AFTER UPDATE ON formoperands
FOR each ROW
BEGIN
  delete formfracs f
   where f.formula_id = :new.formula_id;
  delete calcdata f
   where f.formula_id = :new.formula_id;
END;


SHOW ERRORS 
;

ALTER TABLE "FORMOPERANDS" 
 ADD CONSTRAINT "PK_FORMOPERAND_ID"
	PRIMARY KEY ("FID") USING INDEX
;

