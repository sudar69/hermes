;

CREATE UNIQUE INDEX "UK_PROFDATA_SCD"   
 ON "PROFDATA_SCD" ("DEVICEID","TYPEDATA","ARDTIMESTAMP","PERIOD","ROUND_TYPE_ID","IS_COEF","IS_LOSSES") 
LOGGING
TABLESPACE	USERS
PCTFREE		10
INITRANS	2
MAXTRANS	255
STORAGE (
	INITIAL		28311552
	MINEXTENTS	1
	MAXEXTENTS	2147483645
	BUFFER_POOL	DEFAULT
		)
NOPARALLEL
;

